if [ -e $HOME/.zsh/.zshlocalfunc ]; then
  source $HOME/.zsh/.zshlocalfunc
fi

function ctwi(){  ~/.myscripts/ctwi/ctwi $@ }
function ctl(){   ~/.myscripts/ctl/ctl $@ }
function amv(){ ~/.myscripts/amv/amv $@ }
function cxz(){ ~/.myscripts/cxz/cxz $@ }
function xxz(){ ~/.myscripts/xxz/xxz $@ }
function streamFilter() { ~/.myscripts/streamFilter/streamfilter $@ }
function conv2utf8(){ nkf -w --overwrite $1}
alias doco="~/.myscripts/doco/doco"
alias hexr="~/.myscripts/hexr/hexr"
alias dww="~/.myscripts/dww/dww"
function extlines() { find . -name "*.$1" | xargs wc -l | sort -n }
